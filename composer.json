{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.6",
        "psr/container": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    }
}
